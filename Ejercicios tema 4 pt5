public class Main {
    public static void main (String[] args) {

        String Estacion = "primavera";
        switch (Estacion) {
            case "verano":
                System.out.println("es verano");
                break;
            case "otoño":
                System.out.println("es otoño");
                break;
            case "invierno":
                System.out.println("es invierno");
                break;
            default:
                System.out.println("es primavera");
                break;
        }
    }
}
